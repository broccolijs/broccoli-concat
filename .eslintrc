{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 8
  },
  "env": {
    "node": true
  },
  "globals": {},
  "rules": {
    "no-cond-assign": [
      2,
      "except-parens"
    ],
    "curly": 2,
    "no-debugger": 0,
    "eqeqeq": [
      2,
      "smart"
    ],
    "no-extra-semi": 2,
    "semi": 2,
    "no-eval": 2,
    "guard-for-in": 0,
    "wrap-iife": 0,
    "linebreak-style": 0,
    "new-cap": 2,
    "no-caller": 2,
    "no-empty": 0,
    "no-new": 0,
    "no-plusplus": 0,
    "no-undef": 2,
    "dot-notation": 0,
    "strict": 0,
    "no-unused-vars": 2
  }
}
